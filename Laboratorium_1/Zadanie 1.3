             System.out.printf("%.2f", wynik);
        }
        
        if(planeta.equals("Jowisz")){
             wynik = sekundy/(365.25 * 11.862615 * 24 * 60 * 60);
             System.out.printf("%.2f", wynik);
        }
        
        if(planeta.equals("Saturn")){
             wynik = sekundy/(365.25 * 29.447498 * 24 * 60 * 60);
             System.out.printf("%.2f", wynik);
        }
        
        if(planeta.equals("Uran")){
             wynik = sekundy/(365.25 * 84.016846 * 24 * 60 * 60);
             System.out.printf("%.2f", wynik);
        }
        
        if(planeta.equals("Neptun")){
             wynik = sekundy/(365.25 * 164.79132 * 24 * 60 * 60);
             System.out.printf("%.2f", wynik);
        }
    }
}
