             System.out.printf("%.2f", wynik);
        }  
        
        if(planeta.equals("Wenus")){
             wynik = sekundy/(365.25 * 0.61519726 * 24 * 60 * 60);
             System.out.printf("%.2f", wynik);
        }
        
        if(planeta.equals("Mars")){
