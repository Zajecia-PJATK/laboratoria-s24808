import java.io.*;
import java.util.*;

class ExampleInputOutput {
    public static void main(String[] args) throws IOException {    
        Scanner in = new Scanner(System.in);  
         double sekundy = in.nextDouble();
         String planeta = in.next();
         double wynik;
        
        if(planeta.equals("Ziemia")){
             wynik = sekundy/31557600;
             System.out.printf("%.2f", wynik);
        }
      
        if(planeta.equals("Merkury")){
